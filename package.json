{
  "name": "jsdoc-builder",
  "version": "0.0.6",
  "author": "dori",
  "description": "Generate JSDoc comments for JavaScript, TypeScript, JSX, TSX, and Vue files.",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/klmhyeonwoo/jsdoc-builder"
  },
  "homepage": "https://github.com/klmhyeonwoo/jsdoc-builder#readme",
  "bugs": {
    "url": "https://github.com/klmhyeonwoo/jsdoc-builder/issues"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "default": "./dist/vite.js"
    }
  },
  "bin": {
    "jsdoc-builder": "./dist/cli.js"
  },
  "files": [
    "dist",
    "example"
  ],
  "license": "MIT",
  "keywords": [
    "jsdoc-generator",
    "documentation-generator",
    "auto-jsdoc",
    "typescript-docs",
    "typescript-jsdoc",
    "javascript-documentation",
    "jsx-documentation",
    "tsx-documentation",
    "vue-documentation",
    "react-jsdoc",
    "vue-jsdoc",
    "cli-tool",
    "developer-tools"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "test": "ts-node src/cli.ts"
  },
  "dependencies": {
    "commander": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.34",
    "ts-node": "^10.0.0"
  }
}
